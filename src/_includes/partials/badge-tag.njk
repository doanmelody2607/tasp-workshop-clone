{% for name, length in collections.tagList %}
    <a href="{{ '/tags/' | url }}{{ name }}" class="inline-block px-4 pt-4 pb-2 bg-gray-200 text-gray-800  font-medium rounded-lg prose ">
        {{ name | capitalize }}
        <sup>
            <strong>
                <sup>{{ length }}</sup>
            </strong>
        </sup>
    </a>
{% endfor %}